{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Header from \"../components/Header.vue\";\nimport Sidebar from \"../components/Sidebar.vue\";\nimport LogoutModal from \"@/components/LogoutModal.vue\";\nimport AuthService from \"@/services/auth.service\";\nimport { router } from \"vue-router\";\nimport { ref, computed } from \"vue\";\nexport default {\n  name: \"app\",\n  components: {\n    Header,\n    Sidebar,\n    LogoutModal\n  },\n  setup() {\n    const is_expanded = ref(localStorage.getItem(\"is_expanded\") === \"true\");\n\n    // Calculate dynamic marginLeft value based on is_expanded\n    const mainContentMargin = computed(() => {\n      return is_expanded.value ? `calc(var(--sidebar-width))` : \"4rem\";\n    });\n    const isLogoutModalVisible = ref(false);\n    const showLogoutModal = () => {\n      isLogoutModalVisible.value = true;\n      console.log(\"ok\", isLogoutModalVisible);\n    };\n    const handleLogout = () => {\n      AuthService.logout(); // Call logout method from AuthService to clear localStorage\n      router.push(\"/\"); // Redirect user to login page\n    };\n    return {\n      is_expanded,\n      mainContentMargin,\n      showLogoutModal,\n      handleLogout\n    };\n  }\n};","map":{"version":3,"names":["Header","Sidebar","LogoutModal","AuthService","router","ref","computed","name","components","setup","is_expanded","localStorage","getItem","mainContentMargin","value","isLogoutModalVisible","showLogoutModal","console","log","handleLogout","logout","push"],"sources":["C:\\Users\\Utente\\Desktop\\ShoppableApplication\\admin\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div id=\"Home-page\">\r\n    <Sidebar @show-logout-modal=\"showLogoutModal\" />\r\n    <div class=\"main-content\" :style=\"{ marginLeft: mainContentMargin }\">\r\n      <Header />\r\n      <div class=\"router-view\">\r\n        <router-view />\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <LogoutModal\r\n    :is-logout-modal-visible=\"isLogoutModalVisible\"\r\n    @logout=\"handleLogout\"\r\n  />\r\n</template>\r\n<script>\r\nimport Header from \"../components/Header.vue\";\r\nimport Sidebar from \"../components/Sidebar.vue\";\r\nimport LogoutModal from \"@/components/LogoutModal.vue\";\r\nimport AuthService from \"@/services/auth.service\";\r\nimport { router } from \"vue-router\";\r\nimport { ref, computed } from \"vue\";\r\n\r\nexport default {\r\n  name: \"app\",\r\n  components: {\r\n    Header,\r\n    Sidebar,\r\n    LogoutModal,\r\n  },\r\n\r\n  setup() {\r\n    const is_expanded = ref(localStorage.getItem(\"is_expanded\") === \"true\");\r\n\r\n    // Calculate dynamic marginLeft value based on is_expanded\r\n    const mainContentMargin = computed(() => {\r\n      return is_expanded.value ? `calc(var(--sidebar-width))` : \"4rem\";\r\n    });\r\n\r\n    const isLogoutModalVisible = ref(false);\r\n    const showLogoutModal = () => {\r\n      isLogoutModalVisible.value = true;\r\n      console.log(\"ok\", isLogoutModalVisible);\r\n    };\r\n    const handleLogout = () => {\r\n      AuthService.logout(); // Call logout method from AuthService to clear localStorage\r\n      router.push(\"/\"); // Redirect user to login page\r\n    };\r\n\r\n    return {\r\n      is_expanded,\r\n      mainContentMargin,\r\n      showLogoutModal,\r\n      handleLogout,\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbody {\r\n  background: var(--light);\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-size: 16px;\r\n  font-style: normal;\r\n}\r\n\r\n.main-content {\r\n  flex: 1 1 0; /* Occupy remaining space */\r\n  transition: margin-left 0.2s ease; /* Apply transition to margin-left */\r\n}\r\n.router-view {\r\n  margin-top: 80px;\r\n}\r\n</style>\r\n"],"mappings":";AAgBA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,OAAM,MAAO,2BAA2B;AAC/C,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,WAAU,MAAO,yBAAyB;AACjD,SAASC,MAAK,QAAS,YAAY;AACnC,SAASC,GAAG,EAAEC,QAAO,QAAS,KAAK;AAEnC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVR,MAAM