properties\r\n        this.dotcolor = dot.dotcolor || \"\";\r\n        this.bgcolor = dot.bgcolor || \"\";\r\n        this.textcolor = dot.textcolor || \"\";\r\n\r\n        return {\r\n          text: dot.text || \"\",\r\n          posX: dot.posX || 0,\r\n          posY: dot.posY || 0,\r\n          color: dot.dotcolor || \"\",\r\n          type: \"dot\", // Adjust this as per your requirement\r\n          product: dot.productID || {}, // Ensure `productID` is populated with product details\r\n        };\r\n      });\r\n      console.log(\"Mapped Dots Array:\", mappedDots);\r\n      return mappedDots;\r\n    },\r\n  },\r\n  methods: {\r\n    closeModal() {\r\n      console.log(\r\n        \"this.selectedCollection.dots[0].productID\",\r\n        this.selectedCollection.dots[0].productID\r\n      );\r\n      console.log(this.dotsArr.dots[0].product.name);\r\n      this.$emit(\"close\"); // Émet un événement pour fermer le modal\r\n    },\r\n    calculateTotal(product) {\r\n      return product.quantity * product.price; // Example calculation, adjust as needed\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal-mask {\r\n  background-color: rgb(0, 0, 0, 0.06);\r\n}\r\n.grid-cols-custom {\r\n  grid-template-columns: 1fr 2fr;\r\n}\r\n</style>\r\n"],"mappings":"AA2GA;AACA,OAAOA,QAAO,MAAO,gBAAgB;AACrC,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,UAAU,EAAE;IACVF,QAAQ,CAAE;EACZ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLC,aAAa,EAAEC,OAAO;IAAE;IACxBC,kBAAkB,EAAEC,MAAM,CAAE;EAC9B,CAAC;EACDC,QAAQ,EAAE;IACRC,OAAOA,CAAA,EAAG;MACR,IAAI,CAAC,IAAI,CAACH,kBAAiB,IAAK,CAAC,IAAI,CAACA,kBAAkB,CAACI,IAAI,EAAE;QAC7D,OAAO,EAAE;MACX;MACA,MAAMC,UAAS,GAAI,IAAI,CAACL,kBAAkB,CAACI,IAAI,CAACE,GAAG,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAK;QAClE;QACA,IAAI,CAACd,QAAO,GAAIa,GAAG,CAACb,QAAO,IAAK,EAAE;QAClC,IAAI,CAACC,OAAM,GAAIY,GAAG,CAACZ,OAAM,IAAK,EAAE;QAChC,IAAI,CAACC,SAAQ,GAAIW,GAAG,CAACX,SAAQ,IAAK,EAAE;QAEpC,