{"ast":null,"code":"/* eslint-disable no-unused-vars */\nimport HotSpots from \"./HotSpots.vue\";\nexport default {\n  name: \"InfoCollectionModal\",\n  components: {\n    HotSpots // Register HotSpots as a component\n  },\n  data() {\n    return {\n      dotcolor: \"\",\n      bgcolor: \"\",\n      textcolor: \"\"\n    };\n  },\n  props: {\n    showInfoModal: Boolean,\n    // Prop to control modal visibility\n    selectedCollection: Object // Prop to pass selected product data\n  },\n  computed: {\n    dotsArr() {\n      if (!this.selectedCollection || !this.selectedCollection.dots) {\n        return [];\n      }\n      const mappedDots = this.selectedCollection.dots.map((dot, index) => {\n        // Assign values to dotcolor, bgcolor, and textcolor based on dot properties\n        this.dotcolor = dot.dotcolor || \"\";\n        this.bgcolor = dot.bgcolor || \"\";\n        this.textcolor = dot.textcolor || \"\";\n        return {\n          text: dot.text || \"\",\n          posX: dot.posX || 0,\n          posY: dot.posY || 0,\n          color: dot.dotcolor || \"\",\n          type: \"dot\",\n          // Adjust this as per your requirement\n          product: dot.productID || {} // Ensure `productID` is populated with product details\n        };\n      });\n      console.log(\"Mapped Dots Array:\", mappedDots);\n      return mappedDots;\n    }\n  },\n  methods: {\n    closeModal() {\n      console.log(\"this.selectedCollection.dots[0].productID\", this.selectedCollection.dots[0].productID);\n      console.log(this.dotsArr.dots[0].product.name);\n      this.$emit(\"close\"); // Émet un événement pour fermer le modal\n    },\n    calculateTotal(product) {\n      return product.quantity * product.price; // Example calculation, adjust as needed\n    }\n  }\n};","map":{"version":3,"names":["HotSpots","name","components","data","dotcolor","bgcolor","textcolor","props","showInfoModal","Boolean","selectedCollection","Object","computed","dotsArr","dots","mappedDots","map","