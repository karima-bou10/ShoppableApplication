{"ast":null,"code":"import Header from \"../components/Header.vue\";\nimport Sidebar from \"../components/Sidebar.vue\";\nimport UserSidebar from \"../components/User/UserSidebar.vue\";\nimport { ref, computed } from \"vue\";\nimport AuthService from \"../services/auth.service\";\nexport default {\n  name: \"Home\",\n  components: {\n    Header,\n    Sidebar,\n    UserSidebar\n  },\n  setup() {\n    const isAdmin = AuthService.getUserRole() === \"admin\";\n    const is_expanded = ref(true);\n    const handleToggleExpanded = value => {\n      is_expanded.value = value;\n    };\n    console.log(\"is_expanded home \", is_expanded.value);\n    // Calculate dynamic marginLeft value based on is_expanded\n    const mainContentMargin = computed(() => {\n      console.log(\"is_expanded.value\", is_expanded.value);\n      return is_expanded.value ? `calc(var(--sidebar-width))` : \"4rem\";\n    });\n    return {\n      isAdmin,\n      is_expanded,\n      mainContentMargin,\n      handleToggleExpanded\n    };\n  }\n};","map":{"version":3,"names":["Header","Sidebar","UserSidebar","ref","computed","AuthService","name","components","setup","isAdmin","getUserRole","is_expanded","handleToggleExpanded","value","console","log","mainContentMargin"],"sources":["C:\\Users\\Utente\\Desktop\\ShoppableApplication\\admin\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div id=\"Home-page\">\r\n    <Sidebar v-if=\"isAdmin\" @toggleExpanded=\"handleToggleExpanded\" />\r\n    <UserSidebar v-else @toggleExpanded=\"handleToggleExpanded\" />\r\n    <div class=\"main-content\" :style=\"{ marginLeft: mainContentMargin }\">\r\n      <Header />\r\n      <div class=\"router-view\">\r\n        <router-view />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Header from \"../components/Header.vue\";\r\nimport Sidebar from \"../components/Sidebar.vue\";\r\nimport UserSidebar from \"../components/User/UserSidebar.vue\";\r\nimport { ref, computed } from \"vue\";\r\nimport AuthService from \"../services/auth.service\";\r\n\r\nexport default {\r\n  name: \"Home\",\r\n\r\n  components: {\r\n    Header,\r\n    Sidebar,\r\n    UserSidebar,\r\n  },\r\n\r\n  setup() {\r\n    const isAdmin = AuthService.getUserRole() === \"admin\";\r\n    const is_expanded = ref(true);\r\n    const handleToggleExpanded = (value) => {\r\n      is_expanded.value = value;\r\n    };\r\n\r\n    console.log(\"is_expanded home \", is_expanded.value);\r\n    // Calculate dynamic marginLeft value based on is_expanded\r\n    const mainContentMargin = computed(() => {\r\n      console.log(\"is_expanded.value\", is_expanded.value);\r\n      return is_expanded.value ? `calc(var(--sidebar-width))` : \"4rem\";\r\n    });\r\n\r\n    return {\r\n      isAdmin,\r\n      is_expanded,\r\n      mainContentMargin,\r\n      handleToggleExpanded,\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbody {\r\n  background: var(--light);\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-size: 16px;\r\n  font-style: normal;\r\n}\r\n\r\n.main-content {\r\n  flex: 1 1 0; /* Occupy remaining space */\r\n  transition: margin-left 0.2s ease; /* Apply transition to margin-left */\r\n}\r\n.router-view {\r\n  margin-top: 80px;\r\n}\r\n</style>\r\n"],"mappings":"AAaA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,OAAM,MAAO,2BAA2B;AAC/C,OAAOC,WAAU,MAAO,oCAAoC;AAC5D,SAASC,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,OAAOC,WAAU,MAAO,0BAA0B;AAElD,eAAe;EACbC,IAAI,EAAE,MAAM;EAEZC,UAAU,EAAE;IACVP,MAAM;IACNC,OAAO;IACPC;EACF,CAAC;EAEDM,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIJ,WAAW,CAACK,WAAW,CAAC,MAAM,OAAO;IACrD,MAAMC,WAAU,GAAIR,GAAG,CAAC,IAAI,CAAC;IAC7B,MAAMS,oBAAmB,GAAKC,KAAK,IAAK;MACtCF,WAAW,CAACE,KAAI,GAAIA,KAAK;IAC3B,CAAC;IAEDC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEJ,WAAW,CAACE,KAAK,CAAC;IACnD;IACA,MAAMG,iBAAgB,GAAIZ,QAAQ,CAAC,MAAM;MACvCU,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEJ,WAAW,CAACE,KAAK,CAAC;MACnD,OAAOF,WAAW,CAACE,KAAI,GAAK,4BAA0B,GAAI,MAAM;IAClE,CAAC,CAAC;IAEF,OAAO;MACLJ,OAAO;MACPE,WAAW;MACXK,iBAAiB;MACjBJ;IACF,CAAC;EACH;