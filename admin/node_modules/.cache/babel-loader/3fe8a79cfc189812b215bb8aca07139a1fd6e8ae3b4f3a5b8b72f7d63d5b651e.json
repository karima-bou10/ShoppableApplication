{"ast":null,"code":"import Header from \"../components/Header.vue\";\nimport Sidebar from \"../components/Sidebar.vue\";\nimport LogoutModal from \"../components/LogoutModal.vue\";\nimport { ref, computed } from \"vue\";\nexport default {\n  name: \"app\",\n  components: {\n    Header,\n    Sidebar,\n    LogoutModal\n  },\n  data() {\n    return {\n      logoutModalVisible: false\n    };\n  },\n  methods: {\n    showLogoutModal() {\n      this.logoutModalVisible = true;\n    }\n  },\n  setup() {\n    const is_expanded = ref(localStorage.getItem(\"is_expanded\") === \"true\");\n\n    // Calculate dynamic marginLeft value based on is_expanded\n    const mainContentMargin = computed(() => {\n      return is_expanded.value ? `calc(var(--sidebar-width))` : \"4rem\";\n    });\n    return {\n      is_expanded,\n      mainContentMargin\n    };\n  }\n};","map":{"version":3,"names":["Header","Sidebar","LogoutModal","ref","computed","name","components","data","logoutModalVisible","methods","showLogoutModal","setup","is_expanded","localStorage","getItem","mainContentMargin","value"],"sources":["C:\\Users\\Utente\\Desktop\\ShoppableApplication\\admin\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div id=\"Home-page\">\r\n    <Sidebar @show-logout-modal=\"showLogoutModal\" />\r\n    <LogoutModal v-if=\"logoutModalVisible\" />\r\n    <div class=\"main-content\" :style=\"{ marginLeft: mainContentMargin }\">\r\n      <Header />\r\n      <div class=\"router-view\">\r\n        <router-view />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Header from \"../components/Header.vue\";\r\nimport Sidebar from \"../components/Sidebar.vue\";\r\nimport LogoutModal from \"../components/LogoutModal.vue\";\r\n\r\nimport { ref, computed } from \"vue\";\r\n\r\nexport default {\r\n  name: \"app\",\r\n  components: {\r\n    Header,\r\n    Sidebar,\r\n    LogoutModal,\r\n  },\r\n  data() {\r\n    return {\r\n      logoutModalVisible: false,\r\n    };\r\n  },\r\n  methods: {\r\n    showLogoutModal() {\r\n      this.logoutModalVisible = true;\r\n    },\r\n  },\r\n  setup() {\r\n    const is_expanded = ref(localStorage.getItem(\"is_expanded\") === \"true\");\r\n\r\n    // Calculate dynamic marginLeft value based on is_expanded\r\n    const mainContentMargin = computed(() => {\r\n      return is_expanded.value ? `calc(var(--sidebar-width))` : \"4rem\";\r\n    });\r\n\r\n    return {\r\n      is_expanded,\r\n      mainContentMargin,\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbody {\r\n  background: var(--light);\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-size: 16px;\r\n  font-style: normal;\r\n}\r\n\r\n.main-content {\r\n  flex: 1 1 0; /* Occupy remaining space */\r\n  transition: margin-left 0.2s ease; /* Apply transition to margin-left */\r\n}\r\n.router-view {\r\n  margin-top: 80px;\r\n}\r\n</style>\r\n"],"mappings":"AAaA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,OAAM,MAAO,2BAA2B;AAC/C,OAAOC,WAAU,MAAO,+BAA+B;AAEvD,SAASC,GAAG,EAAEC,QAAO,QAAS,KAAK;AAEnC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVN,MAAM;IACNC,OAAO;IACPC;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACF,kBAAiB,GAAI,IAAI;IAChC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,WAAU,GAAIT,GAAG,CAACU,YAAY,CAACC,OAAO,CAAC,aAAa,MAAM,MAAM,CAAC;;IAEvE;IACA,MAAMC,iBAAgB,GAAIX,QAAQ,CAAC,MAAM;MACvC,OAAOQ,WAAW,CAACI,KAAI,GAAK,4BAA0B,GAAI,MAAM;IAClE,CAAC,CAAC;IAEF,OAAO;MACLJ,WAAW;MACXG;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}